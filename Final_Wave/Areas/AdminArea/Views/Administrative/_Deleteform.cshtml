
<div class="modal" id="myModalReminder">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">

            <!-- Modal Header -->
            <div class="modal-header deletemodel">
                <h4 class="modal-title">
                </h4>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>


            <!-- Modal body -->
            <div class="modal-body ">
                <form asp-controller="Administrative" asp-action="Delete" method="post">
                    <input id="AdministrativeFormID" name="AdministrativeFormID" type="text" hidden />
                    <h5>
                        Are you sure you want to delete this Reminder ? <h6 class="text-center"> ( <span id="AdministrativeFormTitle" name="AdministrativeFormTitle" class="text-primary "></span>)</h6>
                       
                    </h5>

                    <div class="text-right mt-5">
                        <button type="submit" class="btn deletemodel text-white"> Delete  </button>
                        <button type="button" data-dismiss="modal" class="btn btn-outline-primary "> Back </button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
