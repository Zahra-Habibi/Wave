@model IEnumerable<Final_Wave.DataLayer.Entites.About>
@{
    ViewData["Title"] = "Wave software service";
    ViewData["Description"] = "This site is a new site , It provide diffirent software (desktop application , mobile application web application, video and image editing,banner , prototype.).";
    Layout = "_SiteLayout";
    bool IsDelete = false;
}
    <!-- start of upper wave -->
    <div class="upper-wave">
        <img src="~/assets/images/upper-back.png" class="upper-wave img-responsive">
        <h3 class="about-us-title text-center text-white">About US</h3>
    </div>
    <!-- end of upper wave -->
<br><br>
<!-- Start of about company -->
    <div class="container custome-bubles">
                   <!-- bubles -->
                   <div class="bubles"><span class="dot"></span></div>
                   <div class="bubles"><span class="dot"></span></div>
                   <div class="bubles"><span class="dot"></span></div>
                   <div class="bubles"><span class="dot"></span></div>
                   <div class="bubles"><span class="dot"></span></div>
                   <div class="bubles"><span class="dot"></span></div>
                   <div class="bubles"><span class="dot"></span></div>
                   <div class="bubles"><span class="dot"></span></div>
                   <div class="bubles"><span class="dot"></span></div>
                   <div class="bubles"><span class="dot"></span></div>
                   <div class="bubles"><span class="dot"></span></div>
                   <div class="bubles"><span class="dot"></span></div>
                   <div class="bubles"><span class="dot"></span></div>
                   <div class="bubles"><span class="dot"></span></div>
                   <div class="bubles"><span class="dot"></span></div>
                   <!-- end of bubles -->
        <div class="row">
            @foreach(var item in Model){
            <div class="col-sm-12 col-md-6 ">
                <img src="~/@item.Image" alt="@item.Alt" class="img-responsive about-img">
            </div>
            <div class="col-sm-12 col-md-6">
                <h3 class="history-about mt-2">@item.Title</h3>
                <p class="mt-2">
                    @Html.Raw(item.Descriptions)
                </p>
            </div>
            }
        </div>
    </div>
<!-- end of about company -->
    <br>
<br><br>
